{% extends 'consultant/consultant.html.twig' %}

{% block title %}Consultant
{% endblock %}

{% block body %}
	<div class="flex mt-16 consultant-container justify-center">
		<div class="content w-full">
			<div class="flex flex-col items-center ">

				<div class="w-full mt-12 mb-8  bg-gray-100 rounded-full">

					<div class="relative right-0">
						<ul class="tabs relative flex flex-row  list-none rounded-lg bg-blue-gray-50/60 " data-tabs="tabs" role="list">
							<li class="z-30 flex-auto text-center">
								<a id="home" onclick="swithTab(event, 'Home')" class="tablinks text-slate-700 z-30 mb-0 flex w-full cursor-pointer items-center justify-center  border-0 bg-inherit px-0 py-2 transition-all ease-in-out" data-tab-target="" active role="tab" aria-selected="true">
									<span class="ml-1">Home</span>
								</a>
							</li>
							<li class="z-30 flex-auto text-center">
								<a id="profil" onclick="swithTab(event, 'Profil')" class="tablinks text-slate-700 z-30 mb-0 flex w-full cursor-pointer items-center justify-center  border-0 bg-inherit px-0 py-2 transition-all ease-in-out" data-tab-target="" active role="tab" aria-selected="true">
									<span class="ml-1">Profil</span>
								</a>
							</li>
							<li class="z-30 flex-auto text-center">
								<a id="evaluations" onclick="swithTab(event, 'Evaluations')" class="tablinks text-slate-700 z-30 mb-0 flex w-full cursor-pointer items-center justify-center border-0 bg-inherit px-0 py-2 transition-all ease-in-out" data-tab-target="" role="tab" aria-selected="false">
									<span class="ml-1">Evaluations</span>
								</a>
							</li>
							<li class="z-30 flex-auto text-center">
								<a onclick="swithTab(event, 'Missions')" class="tablinks text-slate-700 z-30 mb-0 flex w-full cursor-pointer items-center justify-center  border-0 bg-inherit px-0 py-2 transition-all ease-in-out" data-tab-target="" role="tab" aria-selected="false">
									<span class="ml-1">Missions</span>
								</a>
							</li>
						</ul>
					</div>
				</div>

				<div id="Home" class="tabcontent w-full">
					{% include('consultant/home/home.html.twig') %}
				</div>

				<div id="Profil" class="tabcontent w-full">
					<h3>Profil</h3>
					<p>Open Profil page.</p>
				</div>

				<div id="Evaluations" class="tabcontent w-full">
					<div class="flex flex-wrap">
						{% for test in tests %}
							<div class="flex flex-col justify-between items-center test-block  px-8 shadow-md mr-4 py-8">
								<p class="font-bold text-center">{{ test.nom }}</p>

								<div class="flex flex-row py-2 justify-center">
									{% for i in 1..3 %}
										{% if i <= test.difficulte %}
											<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="star" class="w-5 text-yellow-500 mr-1" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 576 512">
												<path fill="currentColor" d="M259.3 17.8L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0z"></path>
											</svg>
										{% else %}
											<svg aria-hidden="true" focusable="false" data-prefix="far" data-icon="star" class="w-5 text-yellow-500" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 576 512">
												<path fill="currentColor" d="M528.1 171.5L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6zM388.6 312.3l23.7 138.4L288 385.4l-124.3 65.3 23.7-138.4-100.6-98 139-20.2 62.2-126 62.2 126 139 20.2-100.6 98z"></path>
											</svg>
										{% endif %}
									{% endfor %}
								</div>

								<!-- faire le test si le test id pas inclus dans les testResults pour chaque testResult champs idTest-->
								{% set testResultObtained = null %}
								{% for testResult in testResults %}
									{% if testResult.idTest == test.id %}
										{% set testResultObtained = testResult %}
									{% endif %}

								{% endfor %}

